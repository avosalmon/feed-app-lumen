{
    "name": "avosalmon/feed-app-lumen",
    "description": "SNS Feed API",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "kurashicom/response": "0.3.0",
        "danielmewes/php-rql": "dev-master",
        "ktamas77/firebase-php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:kurashicom-laravel-modules/response.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
